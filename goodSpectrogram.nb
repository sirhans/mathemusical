   RowBox[{"sineSweep", "[", 
    RowBox[{"length_", ",", "startF_", ",", "endF_", ",", "sampleRate_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "output", ",", "i", ",", "phase", ",", "frequency", ",", "f1", ",", 
       "f2", ",", "phaseIncrement"}], "}"}], ",", 
     RowBox[{
      RowBox[{"output", "=", 
       RowBox[{"Range", "[", "length", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"phase", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"f1", "=", 
       RowBox[{"N", "[", 
        RowBox[{"2", "\[Pi]", " ", 
         RowBox[{"startF", "/", "sampleRate"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"f2", "=", 
       RowBox[{"N", "[", 
        RowBox[{"2", " ", "\[Pi]", " ", 
         RowBox[{"endF", "/", "sampleRate"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "\[LessEqual]", "length"}], ",", 
        RowBox[{"i", "++"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"output", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "=", 
          RowBox[{"Sin", "[", "phase", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"frequency", "=", 
          RowBox[{"f1", "+", 
           RowBox[{
            RowBox[{"(", 
